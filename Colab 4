","# Prepare data for linear least squares\n","x = sp500_returns.values\n","y = aapl_returns.values\n","y2 = tsla_returns.values\n","\n","# Add a constant term for the intercept\n","A = np.vstack([x, np.ones(len(x))]).T\n","\n","# Perform linear least squares\n","m, b = np.linalg.lstsq(A, y, rcond=None)[0]\n","m2, b2 = np.linalg.lstsq(A, y2, rcond=None)[0]\n","\n","plt.figure(figsize=(20, 10))\n","# Plot the data points and the best-fitting line\n","plt.scatter(x, y, label='Apple', alpha=0.5, color='orange')\n","plt.scatter(x, y2, label='Tesla', alpha=0.5, color='yellow')\n","plt.plot(x, m * x + b, color='orange', label=f'Apple Least Squares Line (Beta: {m:.2f})')\n","plt.plot(x, m2 * x + b2, color='yellow', label=f'Tesla Least Squares Line (Beta: {m2:.2f})')\n","plt.xlabel('S&P 500 Daily Returns')\n","plt.ylabel('AAPL Daily Returns')\n","plt.title('AAPL Returns vs S&P 500 Returns')\n","plt.legend()\n","\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":924},"id":"HjtNFv7ohqOS","executionInfo":{"status":"ok","timestamp":1715883285531,"user_tz":-120,"elapsed":3275,"user":{"displayName":"Nizar Haroun","userId":"11464794240888396180"}},"outputId":"7407cd39-716b-4e48-ae4c-e2482a975672"},"execution_count":13,"outputs":[{"output_type":"stream","name":"stderr","text":["[*********************100%%**********************]  1 of 1 completed\n","[*********************100%%**********************]  1 of 1 completed\n","[*********************100%%**********************]  1 of 1 completed\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 2000x1000 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import yfinance as yf\n","import pandas as pd\n","import numpy as np\n","\n","# Download the list of S&P 500 constituents\n","table = pd.read_html(\"https://en.wikipedia.org/wiki/List_of_S%26P_500_companies\")[0]\n","tickers = table[\"Symbol\"].tolist()\n","\n","# Download historical price data for S&P 